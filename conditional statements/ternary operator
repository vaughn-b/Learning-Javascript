let isLocked = false;

isLocked ? console.log('You will need a key to open the door.') : console.log('You will not need a key to open the door.');
//checks if isLocked is true, and prints first log if it is, and second log if its false
let isCorrect = true;

isCorrect ? console.log('Correct!') : console.log('Incorrect!');
//checks if isCorrect is true, and prints first log if it is, and second log if its false
let favoritePhrase = 'Love That!';

favoritePhrase === 'Love That!' ? console.log('I love that!') : console.log("I don't love that!");
//checks if favoritePhrase is'Love That!' and prints first log if true, and second log if its false
